{
  "name": "tenside/core",
  "description": "Tenside is a wrapper around composer that helps you manage composer projects, either from CLI or browser.",
  "keywords": ["package", "dependency", "autoload"],
  "homepage": "https://github.com/tenside/",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "https://www.cyberspectrum.de"
    }
  ],
  "support": {
    "irc": "irc://irc.freenode.org/tenside",
    "issues": "https://github.com/tenside/core/issues"
  },
  "require": {
    "php": ">=5.5",
    "ext-zip": "*",
    "ext-openssl": "*",
    "composer/composer": "~1.0@dev",
    "composer/semver": "~1.2",
    "firebase/php-jwt": "~2.2",
    "symfony/console": "~3.0",
    "symfony/finder": "~3.0",
    "symfony/http-foundation": "~3.0",
    "symfony/process": "~3.0"
  },
  "require-dev": {
    "phpcq/all-tasks": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "Tenside\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tenside\\Core\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev"
}
